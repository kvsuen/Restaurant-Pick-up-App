  <div id="food-menu" class="light-wrapper">
    <section class="ss-style-top"></section>
    <div class="container inner">
      <h2 class="section-title text-center">Our Menu</h2>
      <p class="lead main text-center"></p>

      <div class="row">
        <div class="col-md-6">
          <!-- left-hand column-->
          <div>
            <table class="table">
              <tbody>
                <tr>
                  <td colspan="3" class="text-center">
                    <span class="sec-title">Mains</span>
                  </td>
                </tr>
                <% for (const item of mains) { %>
                <tr>
                  <td class="menu-text menu-item-<%=(item.id)%>">
                    <span class="dish-name"><%=item.item %></span>
                    </br>
                    <span class="dish-desc"><%=item.description %></span>
                  <td class="price"><%=(item.price / 100) %></td>
                </tr>
                <% } %>
              </tbody>
            </table>
          </div>
        </div>

        <div class="col-md-6">
          <!-- right-hand column-->
          <div>
            <table class="table">
              <tbody>
                <tr>
                  <td colspan="3" class="text-center">
                    <span class="sec-title">Appetizers</span>
                  </td>
                </tr>
                <% for (const item of appetizers) { %>
                <tr>
                  <td class="menu-text menu-item-<%=(item.id)%>">
                    <span class="dish-name"><%=item.item %></span>
                    </br>
                    <span class="dish-desc"><%=item.description %></span>
